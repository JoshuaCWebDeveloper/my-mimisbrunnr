{
  "name": "@my-mimisbrunnr/mimisbrunnr-ext",
  "version": "0.0.3",
  "private": true,
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "development": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "dependencies": {
    "tslib": "^2.3.0"
  },
  "devDependencies": {},
  "nx": {
    "targets": {
      "dev": {
        "executor": "nx:run-commands",
        "defaultConfiguration": "chrome",
        "options": {
          "command": "wxt",
          "cwd": "packages/mimisbrunnr-ext"
        },
        "configurations": {
          "chrome": {
            "command": "wxt -b chrome"
          },
          "firefox": {
            "command": "wxt -b firefox"
          }
        }
      },
      "build": {
        "executor": "nx:run-commands",
        "defaultConfiguration": "chrome",
        "options": {
          "command": "wxt build",
          "cwd": "packages/mimisbrunnr-ext"
        },
        "outputs": [
          "{projectRoot}/dist"
        ],
        "configurations": {
          "chrome": {
            "command": "wxt build -b chrome"
          },
          "firefox": {
            "command": "wxt build -b firefox"
          }
        }
      },
      "zip": {
        "executor": "nx:run-commands",
        "defaultConfiguration": "chrome",
        "options": {
          "command": "wxt zip",
          "cwd": "packages/mimisbrunnr-ext"
        },
        "dependsOn": [
          "build"
        ],
        "configurations": {
          "chrome": {
            "command": "wxt zip -b chrome"
          },
          "firefox": {
            "command": "wxt zip -b firefox"
          }
        }
      },
      "compile": {
        "executor": "nx:run-commands",
        "options": {
          "command": "tsc --noEmit",
          "cwd": "packages/mimisbrunnr-ext"
        }
      },
      "prepare": {
        "executor": "nx:run-commands",
        "options": {
          "command": "wxt prepare",
          "cwd": "packages/mimisbrunnr-ext"
        }
      },
      "lint": {
        "executor": "@nx/eslint:lint",
        "outputs": [
          "{options.outputFile}"
        ]
      },
      "typecheck": {
        "executor": "nx:run-commands",
        "options": {
          "command": "tsc --noEmit",
          "cwd": "packages/mimisbrunnr-ext"
        }
      }
    }
  }
}
